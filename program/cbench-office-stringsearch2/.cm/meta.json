{
  "compile_deps": {
    "compiler": {
      "local": "yes",
      "tags": "compiler,lang-c"
    }
  },
  "compiler_env": "CK_CC",
  "extra_ld_vars": "$<<CK_EXTRA_LIB_M>>$",
  "main_language": "c",
  "process_in_tmp": "yes",
  "run_cmds": {
    "default": {
      "dataset_tags": [
        "doc",
        "txt",
        "dataset"
      ],
      "hot_functions": [],
      "ignore_return_code": "no",
      "run_time": {
        "run_cmd_main": "$#BIN_FILE#$ $#dataset_path#$$#dataset_filename#$ $#dataset_path#$$#dataset_filename_1#$ tmp-output.tmp",
        "run_cmd_out1": "tmp-output1.tmp",
        "run_cmd_out2": "tmp-output2.tmp",
        "run_correctness_output_files": [
          "tmp-output.tmp",
          "tmp-output2.tmp"
        ],
        "run_output_files": [
          "tmp-output.tmp"
        ]
      }
    }
  },
  "run_vars": {
    "CT_REPEAT_MAIN": "1"
  },
  "run_vars_desc": {
    "##CT_REPEAT_MAIN": {
      "default_value": "1",
      "desc_text": "Main function/kernel repetition",
      "explorable": "yes",
      "explore_level": "1",
      "explore_start": "0",
      "explore_step": "10",
      "explore_stop": "1000000",
      "field_size": "7",
      "forbid_disable_at_random": "yes",
      "property": "yes",
      "skip_form_refresh": "yes",
      "sort_index": "1",
      "type": "integer"
    }
  },
  "source_files": [
    "fgg_search.c"
  ],
  "tags": [
    "small",
    "cbench",
    "program",
    "lang-c",
    "crowd-tuning"
  ],
  "target_file": "a"
}
